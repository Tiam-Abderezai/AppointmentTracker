<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.EditAppointment">
   <children>
      <Label layoutX="24.0" layoutY="108.0" prefHeight="34.0" prefWidth="134.0" text="Edit appointment:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="dateStartTextField" layoutX="24.0" layoutY="195.0" promptText="Start Date" />
      <TextField fx:id="dateEndTextField" layoutX="24.0" layoutY="235.0" promptText="End Date" />
      <TextField fx:id="typeTextField" layoutX="24.0" layoutY="275.0" promptText="Type" />
      <Button fx:id="saveCustomer" layoutX="218.0" layoutY="176.0" mnemonicParsing="false" onAction="#saveAppointment" prefHeight="25.0" prefWidth="162.0" text="Save" />
      <Button fx:id="backToMain" layoutX="508.0" layoutY="24.0" mnemonicParsing="false" onAction="#backToMain" text="Main Menu" />
   </children>
</AnchorPane>
