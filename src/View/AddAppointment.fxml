<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddAppointment">
   <children>
      <Label layoutX="14.0" layoutY="98.0" prefHeight="34.0" prefWidth="162.0" text="Create new appointment:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="dateStartTextField" layoutX="14.0" layoutY="185.0" promptText="Start Date" />
      <TextField fx:id="dateEndTextField" layoutX="14.0" layoutY="225.0" promptText="End Date" />
      <TextField fx:id="typeTextField" layoutX="14.0" layoutY="265.0" promptText="Appointment Type" />
      <Button fx:id="addAppointments" layoutX="208.0" layoutY="166.0" mnemonicParsing="false" onAction="#saveAppointment" prefHeight="25.0" prefWidth="162.0" text="Add" />
      <Button fx:id="backToMain" layoutX="498.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMain" text="Main Menu" />
      <TextField fx:id="nameTextField" layoutX="15.0" layoutY="144.0" />
   </children>
</AnchorPane>
